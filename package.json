  "prettier": {
    "semi": false,
    "printWidth": 80,
    "singleQuote": true
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "npm run lint"
    ],
    "*{json, md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:ci"
